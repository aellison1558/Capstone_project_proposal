<h1>Sign Up!</h1>

<%= render 'shared/errors' %>

<form class="form-group" action=<%= users_url %> method="post">
  <%= auth_token %>

  <label for="username">
    Username:
    <input type="string" name="user[username]" value=<%= @user.username %>>
  </label>
  <br/>

  <label for="email">
    Email:
    <input type="string" name="user[email]" value=<%= @user.email %>>
  </label>
    <br/>

  <label for="password">
    Password:
    <input type="password" name="user[password]">
  </label>
    <br/>

  <input type="submit" value="Register!">
</form>
